{
  "name": "sql-migrate-up",
  "version": "0.0.1",
  "description": "Simple SQL Migration tool",
  "main": "build/index.js",
  "types": "build/lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "rollup --bundleConfigAsCjs -c",
    "compile": "tsc --project tsconfig.json --noEmit",
    "compile:watch": "npm run compile -- --watch",
    "lint": "eslint .",
    "release": "npm run test && npm run lint && npm run build && release-it",
    "test": "NODE_ENV=test jest --verbose",
    "test:coverage": "NODE_ENV=test jest --verbose --coverage",
    "test:watch": "NODE_ENV=test jest --verbose --watchAll"
  },
  "keywords": [
    "migration",
    "migrate",
    "migrator",
    "sql",
    "postgresql",
    "snowflake"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/better-sqlite3": "^7.6.3",
    "commander": "^10.0.0",
    "luxon": "^3.3.0",
    "mkdirp": "^2.1.5",
    "save-dev": "^0.0.1-security",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@types/jest": "^29.0.0",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "better-sqlite3": "^8.2.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.6.2",
    "release-it": "^15.0.0",
    "rimraf": "^4.0.0",
    "rollup": "^3.8.0",
    "rollup-plugin-node-externals": "^5.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.0",
    "typescript": "~4.8.0"
  }
}
